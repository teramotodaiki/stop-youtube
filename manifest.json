{
  "manifest_version": 2,
  "name": "Stop Watching Youtube",
  "version": "1.0",
  "description": "YouTube の視聴時間を制限します",
  "icons": {
    "16": "./icons/16.png",
    "32": "./icons/32.png",
    "64": "./icons/64.png",
    "128": "./icons/128.png",
    "256": "./icons/256.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "js": ["youtube.js"]
    }
  ]
}
