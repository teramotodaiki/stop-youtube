{
  "manifest_version": 2,
  "name": "YouTube 制限",
  "version": "1.0",
  "description": "YouTube の視聴時間を制限します",
  "icons": {
    "16": "./icons/16.png",
    "32": "./icons/32.png",
    "48": "./icons/48.png",
    "128": "./icons/128.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "js": ["youtube.js"]
    }
  ],
  "options_page": "./options.html",
  "permissions": ["storage"]
}
